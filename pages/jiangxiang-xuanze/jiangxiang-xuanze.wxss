/* pages/jiangxiang-xuanze/jiangxiang-xuanze.wxss */
.box{
  width: 100%;
  box-sizing: border-box;
  padding-bottom: 160rpx;
}
.list{
  width: 100%;
  background-color: #fff;
}
.list-item{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #f6f6f6;
}
.list-item-nr{
  display: flex;
  align-items: center;
}
.list-item-nr>image{
  width: 66rpx;
  height: 66rpx;
  margin-right: 25rpx;
}
.list-item-nr>text{
  font-size: 30rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.list-item>image{
  width: 33rpx;
  height: 32rpx;
}
/* 按钮 */
.btn{
  width: 100%;
  height:98rpx;
  background:rgba(255,55,51,1);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color:rgba(255,255,255,1);
  position: fixed;
  z-index: 99;
  bottom: 0;
  left: 0;
  right: 0;
}